class Solution:
    def merge(self, nums1: List[int], m: int, nums2: List[int], n: int) -> None:
        """
        Do not return anything, modify nums1 in-place instead.
        """
        # if there are no elements in nums1 just replace 0's by nums2
        # if m==0: 
        #     nums1[:]=nums2
        #     return
        # if n==0: return
        i, j, k = m-1, n-1, m+n-1
        while j>-1:
            if nums1[i]>nums2[j] and i>=0:
                nums1[k] = nums1[i]
                i-=1
            else:
                nums1[k] = nums2[j]
                j-=1
            k-=1
            # if j==-1:
            #     break
            # if i==-1:
            #     if j>-1:
            #         nums1[:k+1] = nums2[:j+1]
            #         break
            #     else:
            #         break
